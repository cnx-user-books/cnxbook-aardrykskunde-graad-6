    <para id="id1167182445294">AARDRYKSKUNDE</para>
    <para id="id1167171721241">Graad 4</para>
    <para id="id1167171718556">HULPBRONNE EN DIENSTE</para>
    <para id="id1167171718132">Module 17</para>
    <para id="id1167171613820">OPVOEDKUNDIGE, HANDELS- EN VERVOERDIENSTE</para>
    <para id="id1167171778258">1. Opvoedkundige dienste</para>
    <para id="id1167171357304"> speelskooltjie</para>
    <para id="id8764525"> kleuterskool</para>
