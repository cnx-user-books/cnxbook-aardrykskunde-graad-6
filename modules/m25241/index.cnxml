    <section id="id1172242012498">
      <title>‘n Grondgebruikkaart </title>
      <figure id="id8440893">
        <media id="id8440893_media" alt="">
          <image mime-type="image/png" src="../../media/graphics1-81af.png" id="id8440893__onlineimage" height="478" width="565"/>
        </media>
